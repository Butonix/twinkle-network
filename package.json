    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.26.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-config-4catalyzer-react": "^0.4.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.2",
