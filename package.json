{
  "name": "twinkle",
  "author": "Mikey Lee <mikey1384@gmail.com>",
  "license": "MIT",
  "version": "1.0.0",
  "repository": "https://github.com/mikey1384/twin-kle.git",
  "description": "Twinkle: educational social network",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_PATH=./source node .",
    "build": "webpack --progress --color -p --config webpack/webpack.prod.js",
    "reload": "sudo pm2 reload index.js",
    "start": "sudo NODE_PATH=./source pm2 start index.js -i max",
    "stop": "sudo pm2 stop index.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/pro-regular-svg-icons": "^5.8.1",
    "@fortawesome/pro-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@hot-loader/react-dom": "^16.8.4",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "brotli-webpack-plugin": "^1.1.0",
    "cross-env": "^5.2.0",
    "d3-ease": "^1.0.5",
    "dotenv": "^7.0.0",
    "emotion": "^10.0.9",
    "emotion-server": "^10.0.9",
    "eslint-plugin-react-hooks": "^1.6.0",
    "exenv": "^1.2.2",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "greenlock-express": "^2.6.8",
    "loadable-components": "^2.2.3",
    "lodash-es": "^4.17.11",
    "moment": "^2.24.0",
    "query-string": "^6.4.0",
    "rc-slider": "^8.6.7",
    "react": "^16.8.5",
    "react-avatar-editor": "^11.0.6",
    "react-dnd": "^7.3.2",
    "react-dnd-html5-backend": "^7.2.0",
    "react-dnd-html5-touch-backend": "^1.5.0",
    "react-dom": "^16.8.5",
    "react-move": "^5.2.1",
    "react-player": "^1.9.3",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-textarea-autosize": "^7.1.0-1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.1.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "stacktrace-js": "^2.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "enzyme": "^3.8.0",
    "eslint": "^5.15.3",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.5.0",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.8.0",
    "regenerator-runtime": "^0.13.2",
    "terser-webpack-plugin": "^1.2.3",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "modulePaths": [
      "<rootDir>/source/"
    ],
    "setupFiles": [
      "<rootDir>/test/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](certs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
